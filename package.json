{
  "name": "ahand",
  "version": "0.1.0",
  "private": true,
  "description": "Cloud AI's local execution gateway",
  "scripts": {
    "dev": "turbo @ahand/dashboard#dev @ahand/dev-cloud#dev @ahand/daemon#dev",
    "build": "turbo build",
    "build:ts": "turbo build --filter=@ahand/proto --filter=@ahand/sdk --filter=@ahand/dev-cloud",
    "build:rust": "turbo build --filter=@ahand/protocol --filter=@ahand/daemon --filter=@ahand/ctl",
    "start": "turbo start",
    "start:cloud": "turbo start --filter=@ahand/dev-cloud",
    "start:daemon": "turbo start --filter=@ahand/daemon",
    "start:ctl": "turbo start --filter=@ahand/ctl",
    "test": "turbo test",
    "lint": "turbo lint",
    "generate": "turbo generate"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "onlyBuiltDependencies": ["@bufbuild/buf", "esbuild"]
  },
  "devDependencies": {
    "turbo": "^2.8.2",
    "typescript": "^5.9.3"
  }
}
