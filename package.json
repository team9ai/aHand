{
  "name": "ahand",
  "version": "0.1.1",
  "private": true,
  "description": "Cloud AI's local execution gateway",
  "scripts": {
    "dev": "turbo @ahand/dashboard#dev @ahand/dev-cloud#dev @ahand/daemon#dev",
    "dev:admin": "turbo @ahand/admin#dev",
    "dev:cloud": "turbo @ahand/dashboard#dev @ahand/dev-cloud#dev",
    "dev:daemon": "turbo @ahand/daemon#dev",
    "configure": "turbo @ahand/ctl#configure",
    "build": "turbo build",
    "build:ts": "turbo @ahand/proto#build @ahand/sdk#build @ahand/dev-cloud#build",
    "build:rust": "turbo @ahand/protocol#build @ahand/daemon#build @ahand/ctl#build",
    "build:admin": "turbo @ahand/admin#build",
    "build:browser": "turbo @ahand/browser-bridge#build",
    "build:release": "turbo @ahand/daemon#build @ahand/ctl#build @ahand/admin#build @ahand/browser-bridge#build",
    "start": "turbo start",
    "start:cloud": "turbo @ahand/dev-cloud#start",
    "start:daemon": "turbo @ahand/daemon#start",
    "start:ctl": "turbo @ahand/ctl#start",
    "start:ctl:configure": "turbo @ahand/ctl#configure",
    "test": "turbo test",
    "test:ts": "turbo @ahand/sdk#test",
    "test:rust": "turbo @ahand/daemon#test @ahand/ctl#test @ahand/protocol#test",
    "test:e2e:scripts": "bash e2e/scripts/run.sh",
    "lint": "turbo lint",
    "lint:ts": "turbo @ahand/proto#lint @ahand/sdk#lint @ahand/dev-cloud#lint",
    "lint:rust": "turbo @ahand/protocol#lint @ahand/daemon#lint @ahand/ctl#lint",
    "generate": "turbo @ahand/proto#generate"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "onlyBuiltDependencies": ["@bufbuild/buf", "esbuild"]
  },
  "devDependencies": {
    "turbo": "^2.8.2",
    "typescript": "^5.9.3"
  }
}
