{
  "name": "@ahand/ctl",
  "version": "0.1.2",
  "private": true,
  "description": "AHand CLI control tool",
  "scripts": {
    "build": "cargo build -p ahandctl",
    "build:release": "cargo build -p ahandctl --release",
    "start": "cargo run -p ahandctl --release",
    "configure": "cargo run -p ahandctl -- configure",
    "lint": "cargo clippy -p ahandctl -- -D warnings",
    "test": "cargo test -p ahandctl"
  }
}
